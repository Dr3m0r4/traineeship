#!/bin/bash

liste=`ls`
if [ $# -ge 1 ]
then
	num=$1
else
	read num
fi

for name in $liste
do
	if [ $name != checkpoint ]
	then
		if [ $name != model.ckpt-$num.data-00000-of-00001 ]
		then
			if [ $name != model.ckpt-$num.index ]
			then
				if [ $name != model.ckpt-$num.meta ]
				then
					rm $name
				fi
			fi
		fi
	fi
done
